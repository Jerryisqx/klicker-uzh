{
  "apps": [
    {
      "name": "klicker-api",
      "script": "src/server.js",
      "wait_ready": true,
      "listen_timeout": 10000,
      "env": {
        "NODE_ENV": "development"
      },
      "env_production": {
        "NODE_ENV": "production"
      },
      "out_file": "/dev/stdout",
      "error_file": "/dev/stderr",
      "watch": false
    }
  ]
}
